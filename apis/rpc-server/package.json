{
  "name": "@pbnjam/rpc-server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build:bin": "bun build ./src/index.ts --compile --outfile ./bin --sourcemap",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "dev": "tsc -w",
    "start": "bun run ./src/index.ts 2>&1 | bunx pino-pretty",
    "start:dev": "bun run --hot ./src/index.ts 2>&1 | bunx pino-pretty"
  },
  "dependencies": {
    "@pbnjam/core": "workspace:*",
    "drizzle-orm": "0.41.0",
    "viem": "2.33.1",
    "express": "^4.18.0",
    "ws": "^8.14.0",
    "cors": "^2.8.5",
    "pino": "^8.0.0",
    "pino-pretty": "^10.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@pbnjam/typescript-config": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "@types/cors": "^2.8.0"
  }
}
