{
  "name": "@pbnjam/telemetry",
  "version": "0.0.1",
  "description": "JIP-3 telemetry implementation for JAM nodes",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist && rm -rf node_modules",
    "check": "tsc --pretty --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix"
  },
  "dependencies": {
    "@pbnjam/core": "workspace:*",
    "@pbnjam/types": "workspace:*",
    "@pbnjam/codec": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "@pbnjam/typescript-config": "workspace:*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "jam",
    "telemetry",
    "jip-3",
    "protocol"
  ],
  "license": "MIT"
}
