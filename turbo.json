{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["CI", "DEBUG", "NODE_ENV", "GITHUB_TOKEN"],
  "globalDependencies": [
    ".eslintignore",
    ".eslint.js",
    ".npmrc",
    ".nvmrc",
    ".prettierignore",
    ".prettierrc.js",
    "tsconfig.json",
    "turbo.json"
  ],
  "tasks": {
    "build": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "tsconfig.build.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "outputs": ["dist/**/*"],
      "dependsOn": ["^build", "^generate"],
      "env": [
      ],
      "cache": true
    },
    "build:bin": {
      "inputs": [
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "outputs": ["bin"],
      "dependsOn": ["^build", "generate"],
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "check": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "dependsOn": ["^build", "generate"],
      "cache": false
    },
    "dev": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "dependsOn": ["^build", "generate"],
      "env": [],
      "cache": false,
      "persistent": true
    },
    "generate": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "outputs": [
        ".mesh/**/*",
        ".graphclient/**/*",
        "src/generated.ts",
        "typechain",
        "migrations/**/*"
      ],
      "cache": false
    },
    "lint": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "dependsOn": ["^build"],
      "outputs": []
    },
    "start": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "dependsOn": ["^build", "generate", "export"],
      "env": [
        "APP_ENV",
        "DATABASE_URL",
        "REDIS_URL",
        "NEXT_PUBLIC_POOLS_API_V0_BASE_URL",
        "POOLS_API_V0_BASE_URL",
        "TOKENS_API_V0_BASE_URL",
        "SWAP_API_V0_BASE_URL",
        "TOKEN_LIST_PR_WEBHOOK_URL",
        "OCTOKIT_KEY",
        "NEXT_PUBLIC_STRAPI_API_URL",
        "NEXT_PUBLIC_ENV_VARIANT",
        "NEXT_PUBLIC_VERCEL_ENV",
        "NEXT_PUBLIC_POSTHOG_KEY",
        "NEXT_PUBLIC_GOOGLE_ANALYTICS"
      ],
      "cache": false,
      "persistent": true
    },
    "start:dev": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "dependsOn": ["^build", "generate", "export"],
      "env": [
        "NEXT_PUBLIC_POSTHOG_KEY",
        "NEXT_PUBLIC_GOOGLE_ANALYTICS"
      ],
      "cache": false,
      "persistent": true
    },
    "test": {
      "inputs": [
        "src/**/*",
        "__tests__/**/*",
        "test/**/*",
        "tests/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "dependsOn": ["^build"],
      "outputs": [],
      "env": []
    },
    "validate": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "cache": true
    },
    "export": {
      "inputs": [
        "src/**/*",
        "package.json",
        "tsconfig.json",
        "infra/**/*",
        "packages/**/*"
      ],
      "cache": true
    },
    "db:migrate": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:migrate:core": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "withdraw:feeVault": {
      "cache": false
    }
  }
}
